{
  "name": "react-hello",
  "version": "1.0.0",
  "description": "A hello world example React app",
  "main": "build/hello.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "lint": "eslint source && eslint examples",
    "build": "npm run clean && browserify -s appClient examples/app-client.js -t babelify > build/app-client.js",
    "watchify": "watchify -s appClient examples/app-client.js -t babelify -o build/app-client.js -v",
    "test": "echo 'No tests defined!'",
    "check": "npm run lint && npm run build && npm test",
    "start": "npm run check && babel-node examples/server.js",
    "validate": "npm run check && npm outdated --depth 0"
  },
  "pre-commit": [
    "validate"
  ],
  "keywords": [
    "react",
    "component",
    "user profile"
  ],
  "authors": [
    "Eric Elliott",
    "Tim Routowicz"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.15",
    "babelify": "^6.1.2",
    "browserify": "^10.2.1",
    "cerebral": "^0.9.0",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.5.2",
    "express": "^4.12.4",
    "pre-commit": "^1.0.7",
    "react": "^0.13.3",
    "watchify": "^3.2.3"
  },
  "dependencies": {
    "lodash": "^3.9.3",
    "prop-types": "^0.1.0",
    "react-stampit": "^0.5.1"
  }
}
